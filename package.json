{
  "name": "@dvinci1/components",
  "version": "0.0.19",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
